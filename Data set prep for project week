      "source": [
        "def clean_auction_title(titles):\n",
        "  for i in range(len(titles)):\n",
        "    titles[i] = titles[i][:-6]\n",
        "    \n",
        "  return titles"
      ],
      "execution_count": 0,
      "outputs": []
