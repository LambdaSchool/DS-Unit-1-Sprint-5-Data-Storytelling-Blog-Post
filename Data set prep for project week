      "source": [
        "for i in range(len(auction_history['Date_Created'])):\n",
        "  \n",
        "  if 'FullFormat' in auction_history['Date_Created'][i]:\n",
        "    auction_history['Date_Created'][i] = auction_history['Date_Created'][i][-4:]\n",
        "    \n",
        "  if 'circa' or 'Circa' in auction_history['Date_Created'][i]:\n",
        "    auction_history['Date_Created'][i] = auction_history['Date_Created'][i][-4:]\n",
        "    \n",
        "  if '-' in auction_history['Date_Created'][i]:\n",
        "    x = auction_history['Date_Created'][i][-2:]\n",
        "    if int(x) > 20:\n",
        "      auction_history['Date_Created'][i] = int(str(19) + str(x))\n",
        "    elif int(x) < 20:\n",
        "      auction_history['Date_Created'][i] = int(str(20) + str(x))\n",
        "      \n",
        "  if auction_history['Date_Created'][i] == '000s':\n",
        "    auction_history['Date_Created'][i] = 2005\n",
        "      \n",
        "  if  int(auction_history['Date_Created'][i]) < 1986:\n",
        "    auction_history['Date_Created'][i] = np.nan\n",
        "    "
      ],
      "execution_count": 0,
      "outputs": []
    },
